{% extends "BackendBundle:Layouts:2column.html.twig" %}

{% block title_section %}
    {{ "Vista de veterinario" | trans }}
{% endblock %}

{% block breadcrumb %}
    {% set crumbs = {'Listado de veterinarios':'#'} %}
    {{ setCrumbs(crumbs) }}
    {{ parent() }}
{% endblock %}

{% block content %}

    <div class="widget-box">
        <div class="widget-title">
            <span class="icon">
                <i class="fa fa-align-justify"></i>
            </span>
            <h5>{{ "usuario"|trans }}</h5>
        </div>
        <div class="widget-content nopadding">
            
                {% if veterinary.name is defined %}{{ veterinary.name }}{% endif %}
                {% if veterinary.last_name is defined %}{{ veterinary.last_name }}{% endif %}
                {{ veterinary.email }}
                {% if veterinary.created is defined %}{{ veterinary.created }}{% endif %}
      
        </div>
    </div>
{% endblock %}